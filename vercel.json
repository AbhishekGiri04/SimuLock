{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/frontend/loading.html"
    },
    {
      "src": "/index",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/about",
      "dest": "/frontend/about.html"
    },
    {
      "src": "/contact",
      "dest": "/frontend/contact.html"
    },
    {
      "src": "/loading",
      "dest": "/frontend/loading.html"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
